#!/bin/bash

# AIE domain for target environment
export AIE_DOMAIN=

# Arize Phoenix specific variables
export PHOENIX_ISTIO_ENDPOINT=https://phoenix.${AIE_DOMAIN}
export PHOENIX_COLLECTOR_ENDPOINT=${PHOENIX_ISTIO_ENDPOINT}/grpc
export PHOENIX_API_KEY=

# Guardrail specific variables
export NEMO_NAMESPACE=agentic
export GUARDRAIL_CONFIG_NAME=demo-self-check-input-output
export GUARDRAILS_BASE_URL=https://nemo-guardrails.${AIE_DOMAIN}/v1/guardrail

# NeMo Deployment Manager variables for main model
export NEMO_MAIN_MODEL_ENGINE=nim
export NEMO_MAIN_MODEL_ID=nvidia/nemotron-3-nano
export NEMO_MAIN_MODEL_URL=
export NEMO_MAIN_MODEL_TOKEN=
export NEMO_MAIN_MODEL_NAME=$(echo ${NEMO_MAIN_MODEL_ID//[^A-z0-9]/_})
export DEPLOYMENT_CONFIG_MAIN_MODEL=$(echo ${NEMO_NAMESPACE}_${NEMO_MAIN_MODEL_NAME} | tr a-z A-Z)

# NeMo Deployment Manager variables for guardrail model
export NEMO_GUARDRAIL_MODEL_ENGINE=nim
export NEMO_GUARDRAIL_MODEL_ID=meta/llama-3.1-8b-instruct
export NEMO_GUARDRAIL_MODEL_URL=
export NEMO_GUARDRAIL_MODEL_TOKEN=
export NEMO_GUARDRAIL_MODEL_NAME=$(echo ${NEMO_GUARDRAIL_MODEL_ID//[^A-z0-9]/_})
export DEPLOYMENT_CONFIG_GUARDRAIL_MODEL=$(echo ${NEMO_NAMESPACE}_${NEMO_GUARDRAIL_MODEL_NAME} | tr a-z A-Z)

# Endpoints for NeMo Deployment Manager and NeMo NIM Proxy
export NIM_ENDPOINT_URL=https://nemo-nim-proxy.${AIE_DOMAIN}
export DEPLOYMENT_BASE_URL=https://nemo-deployment-management.${AIE_DOMAIN}/v1/deployment

# MCP Servers configuration
export MCP_EZPRESTO_SERVER=https://mcp-ezpresto-server.${AIE_DOMAIN}/mcp
export MCP_FILESYSTEM_SERVER=https://mcp-filesystem-server.${AIE_DOMAIN}/mcp
export MCP_S3_SERVER=https://mcp-s3-server.${AIE_DOMAIN}/mcp